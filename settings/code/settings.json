{
    "files.exclude": {
        "**/.husky": true,
        "**/.quasar": true,
        "**/example_mediaPath": true,
        "**/example_resources": true,
        "**/example_tempUploadPath": true,
        "**/node_modules": true,
        "**/dist": true
    },
    "editor.mouseWheelZoom": true,
    "window.title": "${rootName}",
    "telemetry.telemetryLevel": "off",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "i18n-ally.localesPaths": "src/i18n",
    "i18n-ally.pathMatcher": "{locale}/index.{ext}",
    "i18n-ally.includeSubfolders": true,
    "i18n-ally.enabledFrameworks": [
        "vue"
    ],
    "i18n-ally.enabledParsers": [
        "json",
    ],
    "i18n-ally.keystyle": "nested",
    "i18n-ally.indent": 4,
    "i18n-ally.tabStyle": "space",
    "i18n-ally.displayLanguage": "de-DE",
    "editor.cursorBlinking": "smooth",
    "workbench.tree.indent": 18,
    "editor.showFoldingControls": "always",
    "gitlens.telemetry.enabled": false,
    "eslint.format.enable": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "files.eol": "\n",
    "editor.minimap.showSlider": "always",
    "editor.minimap.side": "right",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "search.quickOpen.includeHistory": false,
    "window.autoDetectColorScheme": true,
    "sonarlint.disableTelemetry": true,
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.enablePersistentSessions": false,
    "editor.autoClosingDelete": "never",
    "editor.autoClosingOvertype": "never",
    "editor.codeLens": false,
    "mssql.objectExplorer.groupBySchema": false,
    "sonarlint.rules": {
        "javascript:S3776": {
            "level": "off"
        }
    },
    "editor.fontFamily": "Fira Code Retina",
    "editor.fontLigatures": true,
    "editor.mouseWheelScrollSensitivity": 1.5,
    "volar.autoCompleteRefs": true,
    "notebook.lineNumbers": "on",
    "git.openRepositoryInParentFolders": "never",
    "editor.fontSize": 15,
    "workbench.editor.tabCloseButton": "right",
    "editor.trimAutoWhitespace": false,
    "editor.wordSeparators": "./\\()\"'-:,.;<>~!@#%^&*|+=[]{}`~?",
    "editor.wordWrap": "off",
    "editor.formatOnPaste": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "terminal.integrated.smoothScrolling": true,
    "workbench.list.smoothScrolling": true,
    "editor.smoothScrolling": true,
    "workbench.colorTheme": "Sublime Monokai",
    
    
    
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false
    },
    "editor.acceptSuggestionOnCommitCharacter": true,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.quickSuggestionsDelay": 10,
    "editor.suggestOnTriggerCharacters": true,
    "editor.tabCompletion": "onlySnippets",
    "editor.suggest.localityBonus": true,
    "editor.suggestSelection": "first",
    "editor.wordBasedSuggestions": false,
    "editor.parameterHints.enabled": true,
}